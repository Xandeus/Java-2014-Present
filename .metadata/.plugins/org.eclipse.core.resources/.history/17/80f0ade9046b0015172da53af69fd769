package Chess;

import java.awt.Color;

public abstract class GamePiece {
	int posX, posY;
	Color color;
	public GamePiece(int x, int y,Color color){
		posX = x;
		posY = y;
		this.color = color;
	}
	
	public abstract void move(int x, int y);
	public int getX(){
		return posX;
	}
	public int getY(){
		return posY;
	}
}
