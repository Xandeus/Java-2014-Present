package physicsGame;

public class CannonBall {
	int x, y;
	float vx,vy,ax,ay;
	public CannonBall(int x, int y,float vx,float vy, float ax, float ay){
		this.x = x; 
		this.y = y;
		this.ax = ax; 
		this.ay = ay;
	}
	public int getX(){
		return x;
	}
	public int getY(){
		return x;
	}public float getVX(){
		return x;
	}
	public float getVY(){
		return x;
	}
	}public float getAX(){
		return ax;
	}
	public float getAY(){
		return ay;
	}
}
