package space;

public class Player extends Character{
	int health=4;
	public Player(int x, int y,int w,int h,CelestialBody b){
		super(x,y,w,h,b);	
	}
	public int getHealth(){
		return playerHealth;
	}
	public void setHealth(int x){
		health = x;
	}
	public void decreaseHealth(int x){
		playerHealth-=x;
	}
}
