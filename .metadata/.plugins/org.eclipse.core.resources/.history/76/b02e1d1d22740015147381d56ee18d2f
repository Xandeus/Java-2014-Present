package minesweeper;

public class GameObject {
	private int posX, posY;
	private boolean isHidden = false;
	private boolean isMine;
	public GameObject(int x, int y,boolean isMine){
		posX = x;
		posY = y;
		this.isMine = isMine;
	}
	public boolean isMine(){
		return isMine;
	}
	public boolean isHidden(){
		return isHidden;
	}
	public int getX(){
		return posX;
	}
	public int getY(){
		return posY;
	}
	
}
