package Fractals;

public class Circle {
	int x,y,r;
	boolean active = false;
	public Circle(int x, int y, int r){
		this.x = x;
		this.y = y;
		this.r = r;
		
	}
	public int getX(){
		return x;
	}
	public int getY(){
		return y;
	}
	public int getR(){
		return r;
	}
	public boolean isActive(){
		return active;
	}
	public void changeState(){
		active = !active;
	}
}
