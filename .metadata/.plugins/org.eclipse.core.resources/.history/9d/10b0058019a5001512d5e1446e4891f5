package physicsGame;

public class Player {
	float posX,posY;
	float dX=0,dY=0;
	public Player(int x, int y){
		posX = x;
		posY = y;
	}
	public void setX(int x){
		posX = x;
	}
	public void setY(int y){
		posY = y;
	}
	public void incX(float x){
		dX += x;
	}
	public void incY(float y){
		dY += y; 
	}
}
