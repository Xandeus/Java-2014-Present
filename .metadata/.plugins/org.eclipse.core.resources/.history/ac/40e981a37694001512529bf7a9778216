package space;

import java.awt.Image;
import java.awt.Point;

public class BackgroundStar {
	int size;
	int posX;
	int posY;
	Image image;
	public BackgroundStar(int x, int y, int s,Image i){
		posX = x;
		posY = y;
		size = s;
		image = i;
	}
	public Point getPos(){
		return new Point(posX,posY);
	}
	public int getSize(){
		return size;
	}
	public Image getImage(){
		return Image;
	}
}
