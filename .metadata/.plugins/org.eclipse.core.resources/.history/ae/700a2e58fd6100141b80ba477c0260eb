
public class Box {
	
	int posX,posY,wdth,hght;
	public Box(int startX, int startY, int width, int height) {
		posX = startX;
		posY = startY;
		wdth = width;
		hght = height;
	}
	public int getX(){
		return posX;
	}
	public int getY(){
		return posY;
	}
	public int getWidth(){
		return wdth;
	}
	public int getHeight(){
		return hght;
	}
}
