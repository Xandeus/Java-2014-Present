package physicsGame;

public class CannonBall {
	private int x, y;
	private float vx,vy,ax,ay;
	public CannonBall(int x, int y,float vx,float vy, float ax, float ay){
		this.x = x; 
		this.y = y;
		this.vx = vx; 
		this.vy = vy;
		this.ax = ax; 
		this.ay = ay;
	}
	public CannonBall(int x, int y,float ax, float ay){
		this.x = x; 
		this.y = y;
		this.ax = ax; 
		this.ay = ay;
	}
	public int getX(){
		return x;
	}
	public int getY(){
		return y;
	}public float getVX(){
		return vx;
	}
	public float getVY(){
		return vy;
	}
	public float getAX(){
		return ax;
	}
	public float getAY(){
		return ay;
	}
}
